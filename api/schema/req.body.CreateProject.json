{
    "type": "object",
    "additionalProperties": false,
    "required": [
        "name",
        "source",
        "project_url",
        "tags",
        "access",
        "users",
        "repo"
    ],
    "properties": {
        "name": {
            "type": "string",
            "minLength": 2,
            "maxLength": 40,
            "description": "Human readable Project name"
        },
        "source": {
            "type": "string",
            "minLength": 2,
            "maxLength": 40
        },
        "project_url": {
            "type": "string"
        },
        "tags": {
            "$ref": "./util/project-tags.json"
        },
        "access": {
            "$ref": "./util/project-access.json"
        },
        "notes": {
            "type": "string",
            "default": ""
        },
        "users": {
            "$ref": "./util/project-users.json"
        },
        "repo": {
            "type": "string"
        }
    }
}
