        "json-schema-ref-parser": "^9.0.7",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.95.4",
        "mailgen": "^2.0.14",
        "memjs": "^1.3.0",
        "minimist": "^1.2.2",
        "moment": "^2.27.1",
        "morgan": "^1.10.0",
