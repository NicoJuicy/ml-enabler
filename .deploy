        "docker": [
            "{{project}}:{{gitsha}}",
            "{{project}}:task-build-{{gitsha}}",
            "{{project}}:task-retrain-{{gitsha}}",
            "{{project}}:task-pop-{{gitsha}}",
            "{{project}}:task-vectorize-{{gitsha}}"
        ],
